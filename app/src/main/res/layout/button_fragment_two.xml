<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<TextView
    android:id="@+id/tv_subject_two"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/tv_subject_two"
        android:id="@+id/tv_total_two"
        android:text="TOTAL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_below="@id/tv_subject_two"
        android:layout_toRightOf="@id/tv_total_two"
        android:id="@+id/tv_total_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:text="PRESENT"
        android:id="@+id/tv_present_two"
        android:layout_below="@id/tv_total_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv_present_number"
        android:layout_below="@id/tv_total_two"
        android:layout_toRightOf="@id/tv_present_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
